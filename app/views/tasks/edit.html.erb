<%= link_to("Back to list", tasks_path, class: "back-link")%>

<div class="tasks edit">
    <h2>Edit task</h2>
    <%= form_for(@task) do |f| %>
        <%= render(partial: "form", locals: { f: f })%>
        <div class="form-buttons">
            <%= f.submit("Edit Task")%>
        </div>
    <% end %>
</div>